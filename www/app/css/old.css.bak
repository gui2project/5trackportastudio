/**
 *  @file   style.css
 *
 *  Contains application wide styles.
 */

* {
    margin: 0px;
    padding: 0px;
    border: 0px;
}

/*Background colors*/
body,
button,
.data-not-active {
    background-color: #fafafa;
}

main > .container {
    background-color: #212121;
}

footer {
    background-color: #181818;
}

.dropdown button {
    background: #D8D8D8;
    color: #181818;
    line-height: 30px;
    height: 30px;
}

.panel button,
.data-active {
    background-color: #149BDF;
}

/*font colors*/
main > .container,
footer .copyright,
.panel button {
    color: #fafafa;
}

button {
    color: #181818;
}

main .name input,
input.editable-text {
    color: #212121;
}

main .solo button,
main .mute button,
main .record button,
main .playback button {
    color: #FF007D;
}

.panel .title .fa {
    color: #149BDF;
}

.data-not-active {
    color: #333 !important;
}

.data-active {
    color: #fafafa !important;
}

div.stopWatch {
    border: 1px inset #333;
    background: rgb(20,155,223);
}

div.stopWatch > div.timedisplay {
    font-family: 'Segment7Standard';
    font-size: 40px;
    text-align: center;
    border: 1px inset #333;
    color: #333;
}

.dropdown {
    position: absolute;
    top: 50px;
    left: 0;
    right: 0;
    background: #181818;
    z-index: 1000;
    padding: 0;
    color: #fafafa;
    border-bottom-left-radius: 10px;
    border-bottom-right-radius: 10px;
    border-bottom: outset 1px #333;
}

.dropdown .holder {
    overflow-y: auto;
    height: 700px;
    float: left;
    width: 100%;
    margin: 0;
    padding: 0;
}

.dropdown-left, .dropdown-right {
    height: auto;
    float: left !important;
}

.panel {
    background: none;
    border: none;
    -webkit-box-shadow: none;
    box-shadow: none;
    margin-top: 10px;
    float: none;
}

.panel .title .fa {
    line-height: 20px;
    font-size: 15px;
    margin-right: 5px;
}

.fx-catalog-panel,
.account-panel,
.mix-catalog-panel,
.login-panel,
.register-panel,
.information-panel,
.about-panel,
.contact-panel,
.api-panel,
.dropdown
.tutorial-panel {
    display: none;
}

.wait {
    background-image: url('/app/img/wait.gif');
    background-repeat: no-repeat;
    background-position: center center;
    height: 100%;
    display: none;
}

.wait-left-panel {
    float: left;
}

.wait-right-panel {
    float: left;
}

.dropdown .row {
    margin-left: 0;
}

.dropdown-left {
    float: left !important;
}

.api-item-url {
    font-size: 16px;
    color: #149BDF;
}

.api-item-param {
    font-style: italic;
    margin-right: 25px;
    width: 40px;
    display: inline-block;
    color: #FF007D;
}

.api-row-tabbed {
    margin-left: 25px;
}

.api-row-options {
    margin-left: 90px;
}

.api-url-param {
    color: #FF007D;
}

.api-row-options {
    color: #149BDF;
}

.api-url-param {
    font-style: italic;
}

.api-item-param,
.api-item-param-description {
    font-size: 14px;
}

div.track {
    text-align: center;
}

header {
    z-index: 1001;
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    border-bottom: 0;
    height: 20px;
}

.navbar-default {
    background-color: #181818;
    border: 0;
}

.navbar-default .navbar-brand {
    color: #ffffff;
}

.navbar-brand-label,
.splash-brand-label {
    height: 40px;
    background-image: url('/app/img/favicon-white.png');
    background-repeat: no-repeat;
    background-position: left 0;
    padding-left: 26px;
    background-size: 22px 22px;
    margin-left: 5px;
    color: #FAFAFA;
}

div.navbar-right-links a {
    padding: 0 0 0 10px;
    height: 50px;
    line-height: 50px;
    float: right;
}

div.navbar-right-links a.link-sign-in span,
div.navbar-right-links a.link-register span,
div.navbar-right-links a.link-sign-out span,
.navbar-info-display,
.navbar-header > a > span {
    height: 38px;
    line-height: 55px;
    color: #FAFAFA;
    float: right;
}

.navbar-brand-label:hover,
span.navbar-brand-label:hover,
div.navbar-right-links > a > span:hover {
    color: #149BDF;
    ;
    text-decoration: none;
    border-bottom: 1px solid #149BDF;
}

.catalog-label {
    height: 50px;
    line-height: 50px;
    margin-left: 10px;
    text-align: center;
    float: left;
    padding: 0 10px;
}

.fx-catalog-label {
    background: #FAFAFA;
}

.mix-catalog-label {
    background: #FAFAFA;
}

.navbar-right-links {
    float: right;
    text-align: right;
}

.navbar-info-display {
    height: 35px;
    line-height: 55px;
    color: #FAFAFA;
    float: left;
}

a.link-register,
a.link-sign-in,
a.link-sign-out {
    display: none;
}
/*Positioning*/
html,
body {
    width: 100%;
    height: 100%;
}

body,
.sys-font {
    font-family: 'Open Sans', sans-serif;
    /*Calibri, Candara, Segoe, 'Segoe UI', Optima, Arial, sans-serif;*/
    font-weight: 300;
}


/* Main mixer styling */

main > .container {
    border-radius: 10px;
    margin-top: 60px;
}

main .container .row {
    padding: 8px 0;
}

main .container .row:first-child {
    padding: 8px 0;
    border-top: none;
}

main .row-container {
    padding: 0;
    margin: 0;
    margin-right: -15px;
    margin-left: -15px;
}

main .name {
    margin: auto;
    font-size: 20px;
    text-align: center;
}

main .name input {
    margin: auto;
    text-align: center;
}

main .eq,
main .pan {
    width: 98px;
    height: 80px;
    margin: auto;
    position: relative;
}

main .eq span,
main .pan span {
    width: 20px;
    margin: auto;
}

main .eq label,
main .pan label {
    text-align: center;
    font-size: 12px;
    width: 40px;
    height: 20px;
    position: absolute;
    left: 27px;
    bottom: 25px;
    padding: 0;
    margin: 0;
}

main .solo,
main .mute,
main .record {
    float: left;
    margin: 0;
    padding: 0;
}

main .solo button,
main .mute button,
main .record button {
    width: 90%;
    height: 40px;
    font-weight: bold;
}


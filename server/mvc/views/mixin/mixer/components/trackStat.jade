mixin trackStat(id)
    tr
        td.col-xs-8.col-sm-8.col-md-8.col-lg-8.name-label(id='track-name-display-'+id)
        td.col-xs-4.col-sm-4.col-md-4.col-lg-4.length-label(id='track-time-display-'+id)
            .col-xs-1.col-sm-3.col-md-1.sys-font
            .col-xs-3.col-sm-1.col-md-3.sys-font 000
            .col-xs-1.col-sm-1.col-md-1.sys-font
            .col-xs-2.col-sm-2.col-md-2.sys-font 00
            .col-xs-1.col-sm-1.col-md-1.sys-font
            .col-xs-3.col-sm-1.col-md-3.sys-font 000
            .col-xs-1.col-sm-3.col-md-1.sys-font
